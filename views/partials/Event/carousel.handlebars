{{#each events}}
<div id="eventview{{issue_code.Data.dataValues.ID}}" class="carousel slide my-4" data-ride="carousel" style="background-color: var(--Background-color);">
  <div class="carousel-inner">
      {{#if (isNull solution_code.Code)}}
        <div class="carousel-item  active">
          <div class="row d-flex justify-content-center">
            <div class="col-6 text-center" >
              <span>ERRO </span>
              <span>ID:?</span>
            </div>
            <div class="col-8 col-sm-6">
              <span>Linguagem: ?</span>
            </div>
            <div class="col-8">
              <pre><code class="language-c">?</code></pre>
            </div>
          </div>
        </div>
      {{else}}
        <div class="carousel-item  active">
          <div class="row d-flex justify-content-center">
            <div class="col-6 text-center" >
              <span>ERRO </span>
              <span>ID:{{solution_code.Data.dataValues.ID}}</span>
            </div>
            <div class="col-8 col-sm-6">
              <span>Linguagem: {{solution_code.Language.dataValues.Name}}</span>
            </div>
            <div class="col-8">
              <pre><code class="language-{{solution_code.Language.dataValues.Name}}">{{solution_code.Code.dataValues.Code}}</code></pre>
            </div>
          </div>
        </div>
      {{/if}}
        {{#if (isNull issue_code.Code)}}
        <div class="carousel-item ">
          <div class="row d-flex justify-content-center">
            <div class="col-6 text-center" >
              <span>SOLUÇÃO </span>
              <span>ID:?</span>
            </div>
            <div class="col-8 col-sm-6">
              <span>Linguagem: ?</span>
            </div>
            <div class="col-8">
              <pre><code class="language-c">?</code></pre>
            </div>
          </div>
        </div>
      {{else}}
        <div class="carousel-item ">
          <div class="row d-flex justify-content-center">
            <div class="col-6 col-lm text-center" >
              <span>SOLUÇÃO </span>
              <span>ID:{{issue_code.Data.dataValues.ID}}</span>
            </div>
            <div class="col-8 col-sm-6">
              <span>Linguagem: {{issue_code.Language.dataValues.Name}}</span>
            </div>
            <div class="col-8 row">
              <div class="col-12">
                <pre><code class="language-{{issue_code.Language.dataValues.Name}} col-12">{{issue_code.Code.dataValues.Code}}</code></pre>
              </div>
              <div class="col-12 my-2">
                <input type="number" class="d-none" value="{{issue_code.Data.dataValues.ID}}">
                <button class="btn" style="background-color: var(--accept-color);border-color: var(--accept-color);color: #ffff">Sobre</button>
              </div>
            </div>
          </div>
        </div>
      {{/if}}
  </div>
  <a class="carousel-control-prev" href="#eventview{{issue_code.Data.dataValues.ID}}" role="button" data-slide="prev" style="background-color: var(--main-color);">
    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    <span  class="sr-only">Previous</span>
  </a>
  <a class="carousel-control-next" href="#eventview{{issue_code.Data.dataValues.ID}}" role="button" data-slide="next" style="background-color: var(--main-color);">
    <span class="carousel-control-next-icon" aria-hidden="true"></span>
    <span class="sr-only">Next</span>
  </a>
</div>
{{/each}}