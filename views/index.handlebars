{{>navbar }}
<div class="container mt-4" style="display:flex;flex-wrap: wrap;flex-direction: row;justify-content: space-between;">
    {{#each antip}}
        <div class="card mt-2" style="width: 18rem;">
            <div class="card-body">
                <h5 class="card-title">{{err_num}}</h5>
                <h6 class="card-subtitle mb-2 text-muted">Erro: {{err_nom}}</h6>
                <p class="card-text"></p>
                <pre><code class="language-c">{{code}}</code></pre>
            </div>
        </div>
    {{else}}

    {{/each}}
</div>